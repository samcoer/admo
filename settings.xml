<settings>
  <profiles>
    <profile>
        <id>admo</id>
        <properties>
        </properties>

        <activation>
            <activeByDefault>true</activeByDefault>
        </activation>

        <repositories>
			<repository>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
			<id>central</id>
			<name>Maven Repository Switchboard</name>
			<url>http://repo1.maven.org/maven2</url>
			</repository>
		</repositories>

        <pluginRepositories>
			<pluginRepository>
			  <releases>
				<updatePolicy>never</updatePolicy>
			  </releases>
			  <snapshots>
				<enabled>false</enabled>
			  </snapshots>
			  <id>central</id>
			  <name>Maven Plugin Repository</name>
			  <url>http://repo1.maven.org/maven2</url>
			</pluginRepository>
        </pluginRepositories>
    </profile>
  </profiles>
  
	<pluginGroups>
	    <pluginGroup>com.atlassian.maven.plugins</pluginGroup>
	</pluginGroups>   
  
</settings>
